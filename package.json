{
  "name": "react-translator",
  "description": "React component for translation on the fly",
  "version": "0.1.0",
  "author": {
    "name": "D3kion",
    "url": "https://github.com/D3kion"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/D3kion/react-translator"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.cjs",
  "module": "./dist/my-lib.js",
  "exports": {
    ".": {
      "types": "./dist/react-translator.d.ts",
      "import": "./dist/react-translator.js",
      "require": "./dist/react-translator.cjs"
    },
    "./adapters": {
      "types": "./dist/react-translator-adapters.d.ts",
      "import": "./dist/react-translator-adapters.js",
      "require": "./dist/react-translator-adapters.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build"
  },
  "packageManager": "bun@1.2.18",
  "peerDependencies": {
    "typescript": "^5.8.3",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "google-translate-api-x": "^10.7.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "vite": "^7.0.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
